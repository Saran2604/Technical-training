#include <stdio.h>

int main()
{
    int num, i;
    printf("ENTER THE NUMBER:");
    scanf("%d",&num);
    for(i=1;i<=num;i++){
        if(i%2==0){
            printf("%d it is Even number\n",i);
        }
        else{
            printf("%d it is odd number\n",i);
        }
    }

    return 0;
}
