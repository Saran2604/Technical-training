#include <stdio.h>

int main()
{
    int i,num;
    printf("Enter the Number\n");
    scanf("%d",&num);
    
    for(i=1;i<=num;i++)
    {
    if (i %2==0)
    {
        printf("%dThe number is Even \n",i);
    }
    else
    {
        printf("%dThe number is Odd \n",i);
    }
        
    }
    
    
   
    return 0;
}
