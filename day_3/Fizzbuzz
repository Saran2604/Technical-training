#include <stdio.h>

int main()
{
    int num, i;
    printf("ENTER THE NUMBER:");
    scanf("%d",&num);
    for(i=1;i<=num;i++){
        if(i%3==0&&i%5==0)
        {
            printf("%d it is fuzzbuzz\n",i);
        }
        else if(i%3==0){
            printf("%d it is fuzz\n",i);
        }
        else if(i%5==0){
            printf("%d it is buzz\n",i);
            
        }
        else{
            printf("%d\n",i);
            ]
        }
        
        
    }

    return 0;
}
